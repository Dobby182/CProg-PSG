#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    char *string;
     string=malloc(1024*sizeof(char));
     scanf("%s",string);
     int frequencies[10]={0};
     
     int i=0;
    while(string[i]!='\0'){
        if(isdigit(string[i])){
            int digit=string[i]-'0';
            frequencies[digit]++;
            
        }
        
        i++;
            
    }
    // Print the frequencies separated by spaces
    for (int j = 0; j < 10; j++) {
        printf("%d ", frequencies[j]);
    }
    printf("\n");

    free(string);
    return 0;
     
     }
     
    

